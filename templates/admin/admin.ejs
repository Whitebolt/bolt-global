<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta http-equiv="x-ua-compatible" content="ie=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Bolt Admin</title>

	<link rel="stylesheet" href="/css/admin.css">
</head>
<body>
	<nav class="top-bar">
		<div class="top-bar-left">
			<ul class="dropdown menu" data-dropdown-menu>
				<li>
					<a href="/admin">Admin</a>
					<ul class="menu vertical">
						<li><a href="/admin/pages">Pages</a></li>
						<li><a href="/admin/menus">Menus</a></li>
						<li><a href="/admin/users">Users</a></li>
					</ul>
				</li>
			</ul>
		</div>

		<div class="top-bar-right">
			<ul class="dropdown menu" data-dropdown-menu>
				<li class="menu-text">Bolt Admin <%- req.app.config.version %></li>
			</ul>
		</div>
	</nav>
	<article class="row expanded">
		<div class="columns small-3 selector-menu">
		<% if (doc && doc.view) {%>
			<%- view(doc.view, doc, req, parent) %>
		<% } %>
		</div>
		<div class="columns small-9 editor">
		<% if (doc && doc.viewEditor) {%>
			<%- view(doc.viewEditor, doc, req, parent) %>
		<% } %>
		</div>
	</article>
</body>
<script src="/lib/jquery/dist/jquery.min.js"></script>
<script src="/lib/jqte/jQuery-TE_v.1.3.2/jquery-te-1.3.2.min.js"></script>
<script src="/lib/angular/angular.min.js"></script>
<script src="/lib/what-input/what-input.min.js"></script>
<script src="/lib/foundation-sites/dist/foundation.min.js"></script>
<script src="/scripts/admin.min.js"></script>
</html>